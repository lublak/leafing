{
  "components": [
    {
      "type": "header",
      "text": "#title",
      "guard": "#title->exists",
      "x": -1,
      "y": -1
    },
    {
      "type": "separator",
      "guard": "#showSeparator",
      "x": -1,
      "y": -1
    },
    {
      "type": "text",
      "text": "Taming Item:",
      "guard": "#taming1->iexists",
      "x": 0,
      "y": 30
    },
    {
      "type": "item",
      "item": "#taming1",
      "guard": "#taming1->iexists",
      "x": 0,
      "y": 40
    },
    {
      "type": "item",
      "item": "#taming2",
      "guard": "#taming2->iexists",
      "x": 20,
      "y": 40
    },
    {
      "type": "item",
      "item": "#taming3",
      "guard": "#taming3->iexists",
      "x": 40,
      "y": 40
    },
    {
      "type": "item",
      "item": "#taming4",
      "guard": "#taming4->iexists",
      "x": 60,
      "y": 40
    },
    {
      "type": "item",
      "item": "#taming5",
      "guard": "#taming5->iexists",
      "x": 80,
      "y": 40
    },
    {
      "type": "item",
      "item": "#taming6",
      "guard": "#taming6->iexists",
      "x": 100,
      "y": 40
    },
    {
      "type": "text",
      "text": "Taming:$(br)#custom_taming#",
      "guard": "#custom_taming->exists",
      "x": 0,
      "y": 30
    },
    {
      "type": "text",
      "text": "Infos:$(br)#infos#",
      "guard": "#infos->exists",
      "x": 0,
      "y": 60
    }
  ]
}